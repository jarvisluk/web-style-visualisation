{
  "id": "claymorphism",
  "name": "Claymorphism",
  "nameZh": "黏土态",
  "category": "modern",
  "description": "Soft 3D clay-like elements with rounded shapes and inner shadows",
  "descriptionZh": "圆润柔和的 3D 黏土质感，内外双阴影",
  "author": "core",
  "references": ["https://hype4.academy/tools/claymorphism-generator"],
  "variables": {
    "--color-primary": "#6366f1",
    "--color-primary-hover": "#4f46e5",
    "--color-bg": "#f1f5f9",
    "--color-surface": "#ffffff",
    "--color-text": "#334155",
    "--color-text-secondary": "#64748b",
    "--color-accent": "#f43f5e",
    "--color-border": "transparent",
    "--border-radius": "32px",
    "--border-width": "0px",
    "--border-color": "transparent",
    "--shadow-x": "10px",
    "--shadow-y": "10px",
    "--shadow-blur": "20px",
    "--shadow-spread": "0px",
    "--shadow-color": "rgba(148, 163, 184, 0.35)",
    "--font-family": "'Inter', sans-serif",
    "--font-weight": "500",
    "--font-weight-bold": "700",
    "--backdrop-blur": "0px",
    "--bg-opacity": "1"
  },
  "specialTuning": [
    {
      "variable": "--border-radius",
      "label": "Roundness",
      "labelZh": "圆润度",
      "type": "range",
      "min": 8,
      "max": 40,
      "step": 2,
      "unit": "px"
    }
  ],
  "keyProperties": [
    { "property": "border-radius: 24px", "explanation": "大圆角模拟黏土的圆润体块" },
    { "property": "box-shadow (inner+outer)", "explanation": "内外双阴影创造 3D 立体质感" },
    { "property": "warm palette", "explanation": "温暖色调增强黏土/手工感" }
  ]
}
